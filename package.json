    "compression": "^1.6.0",
    "cookie-parser": "^1.4.1",
    "core-decorators": "^0.12.2",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "express-session": "^1.13.0",
    "history": "^1.17.0",
    "humps": "^0.6.0",
    "install": "^0.7.3",
    "isomorphic-fetch": "^2.1.1",
    "lodash": "^3.10.1",
    "material-ui": "^0.15.0",
    "method-override": "^2.3.5",
    "moment": "^2.13.0",
    "mongojs": "^2.3.0",
    "normalizr": "^1.0.0",
    "password-hash": "^1.2.2",
